;	ax	[fg2 swap=鑑定官0 fn=女子中_惚れる]@
1	[fg2 fn=鑑定官_眼鏡]@
2	[fg2 fn=女子中_惚れる]@
屈	[fg2_squat name=女子中]@
震	[fg2_shake name=女子中]@
;横跳	[fg2_sidestep name=女子中]@
横跳	[fg2_sidestep name=女子中 no_wait=true]@
